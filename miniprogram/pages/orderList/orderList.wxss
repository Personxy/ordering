.vtabs-content-item .item {
  width: 100%;
  padding-bottom: 40rpx;
  display: flex;
  padding-left: 10rpx;
}

.item .item-image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8rpx;
}
.title {
  font-weight: 600;
  font-size: 28rpx;
  text-align: left;
}
.item .item-info {
  margin-left: 20rpx;
  flex: 1;
  position: relative;
}
.item-info .add-icon {
  position: absolute;
  bottom: 0rpx;
  right: 30rpx;
}
